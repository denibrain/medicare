<h1>Listing scans</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Scantype</th>
    <th>Treatment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scans.each do |scan| %>
  <tr>
    <td><%= scan.name %></td>
    <td><%= scan.scantype_id %></td>
    <td><%= scan.treatment_id %></td>
    <td><%= link_to 'Show', scan %></td>
    <td><%= link_to 'Edit', edit_scan_path(scan) %></td>
    <td><%= link_to 'Destroy', scan, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Scan', new_scan_path %>
